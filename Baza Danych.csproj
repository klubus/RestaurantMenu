<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Baza_Danych</RootNamespace>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\addIcon.png" />
    <None Remove="Assets\backIcon.png" />
    <None Remove="Assets\editIcon.png" />
    <None Remove="Assets\menu.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\addIcon.png" />
    <Resource Include="Assets\backIcon.png" />
    <Resource Include="Assets\editIcon.png" />
    <Resource Include="Assets\menu.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
